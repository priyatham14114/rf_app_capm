                                class="buttonContainer"
                                justifyContent = "SpaceBetween"
                               
                            >
                                <Button
                                    id="idChngeF4"
                                    text="PosAuto"
                                    press=""
                                />
                                <Button
                                    id="idChngeF5"
                                    text="PosMan"
                                    press="PosMan"
                                />
                                <Button
                                    id="idChngeF6"
                                    text="Equipment Unloading"
                                    press="onUnLoadBtnPress"
                                />
                            </HBox>
