                            <Label
                                id="idUOMabel_DA"
                                text="OWTs:"
                                class="classUOMbyASn"
                            />
                            <Input
                                id="idUOMInput_DA"
                                width="60px"
                                editable="false"
                            />
                        </HBox>
                        <HBox width="10px"></HBox>
                        <HBox
                            id="idHubEVTHUuomboxDeconsolidationAutomatically"
                            class="customLabelSpacing"
                        >
                            <Label
                                id="idEVTabel_DA"
                                text="TVHU:"
                                class="classUOMbyASn"
                            />

                            <Input
                                id="idEVTInput_DA"
                                width="40px"
                                editable="false"
                            />
                            <Input
                                id="idPERInput_DA"
                                value="L"
                                width="10px"
                                editable="false"
                            />
                        </HBox>
                    </HBox>

                    <Label
                        id="idWidthLabelDeconsolidationAutomatically"
                       

                    />
                    <HBox id="hboxx3_DA">
                        <Input
                            id="idWidthInputDeconsolidationAutomatically"
                            placeholder="Width"
                            maxLength="25"
                            width="150px"
                            editable="false"
                        />
                        <HBox width="5px"></HBox>
                         <Input
                            id="idWidthInputDeconsolidationAutomatically1"
                            placeholder="Width"
                            maxLength="25"
                            width="100px"
                            editable="false"
                        />
                  
                    </HBox>
  
 <Label id="idEmptyLabelForSpacing" text=""/>
<VBox id="idVBoxForTextAndButtons">
    <Text id="idClosePutawayText" text="Close putaway HU?" />
    <HBox id="idHBoxForButtons">
        <Button id="idYesButton" text="Yes" />
        <Label id="idButtonSpacingLabel" width="20px"/> 
        <Button id="idNoButton" text="No" />
    </HBox>
</VBox>


                 
 
    
                  
                </f:SimpleForm>
               
            </ScrollContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
