                    alignItems="Center"
                >
                    <Button
                        id="idNavBackButton_3DeconsolidationAutomatically"
                        icon="sap-icon://nav-back"
                        press="Onpressback3"
                    />
                    <Title
                        id="idTitle1DeconsolidationAutomatically_page4"
                       
                        class="TitleSecondPage"
                        level="H1"
                        textAlign="Center"
                    />
                    <!-- This empty Text element helps push the Title to the center -->
                    <Text
