                                text="Unit:"
                                class="classUOMbyASn"
                                
                            />
                            <Input
                                id="idDAUInput"
                                width="20px"
                                 value="KG"
                                 editable="false"
                            />
                        </HBox>
                    </HBox>
                  
                    <Label
                        id="idLLabelDeconsolidationAutomatically"
                        text="DGs"
                    />
                    <HBox id="hboxx2_DA">
                        <Input
                            id="idLInputDeconsolidationAutomatically"
                            placeholder=""
