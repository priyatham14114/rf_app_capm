>
    <tnt:ToolPage id="idToolPageUnloadingByBillofLading">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeaderUnloadingByBillofLading_UOBL">
                <Image
                    id="_IDGenImage1UnloadingByBillofLading_UOBL"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparatorUnloadingByBillofLading_UOBL"
                />
                <ToolbarSpacer id="idToolbarSpacerUnloadingByBillofLading_UOBL">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData1UnloadingByBillofLading_UOBL"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar1UnloadingByBillofLading_UOBL"
                    displaySize="XS"
                    press="onAvatarPressed"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>

        <tnt:mainContents>
            <ScrollContainer
                id="page1billofLading_UOBL"
                width="100%"
                height="100%"
                horizontal="false"
                vertical="true"
                class="sapUiContentPadding"
                visible="true"
            >
                <Button
                    id="idNavBackButton0_UOBL"
                    icon="sap-icon://nav-back"
                    press="Onpressback0"
                />

                <VBox
                    id="vBox_BOL_UOBL"
                    alignItems="Center"
                    justifyContent="Center"
                    class="centerContent"
                >
                    <Title
                        id="mainfirstpageTitle_UBOL"
                        text="Unloading By Bill of Lading"
                        class="TitleSecondPage"
                        level="H2"
                    />
                    <f:SimpleForm
                        id="simpleFormBOL_UOBL"
                        layout="ResponsiveGridLayout"
                        editable="true"
                        class="sapUiResponsiveMargin"
                        labelSpanL="3"
                        labelSpanM="3"
                        emptySpanL="4"
                        emptySpanM="4"
                        columnsL="1"
                        columnsM="1"
                    >
                        <HBox
                            id="idHBox1_UOBL"
                            justifyContent="Start"
                        >
                            <Label
                                id="idBillofLadingLabel_UOBL"
                                text="Bill of Lading :"
                                class="ScanHumargin"
                            />
                            <!-- Input Field -->
                            <Input
                                id="idBillofLadinginput_UOBL"
                                placeholder="Enter HU..."
                                class="sapUiTinyMarginBegin"
                            />
                            <!-- Barcode Scan Button -->
                            <ndc:BarcodeScannerButton
                                id="sampleBarcodeScannerButtonZebraOnebilloflading_UOBL"
                                scanSuccess="onScanSuccessOne"
                                scanFail="onScanErrorOne"
                                inputLiveUpdate="onScanLiveUpdate"
                                dialogTitle="Enter HU Number"
                                class="sapUiTinyMarginBegin"
                            />
                            <!-- Submit Button -->
                            <Button
                                id="idButtonSubmit_UOBL"
                                text="Submit"
                                press="Onpresssubmit"
                            />
                        </HBox>
                    </f:SimpleForm>
                </VBox>
            </ScrollContainer>

            <ScrollContainer
                id="page2billofLading_UOBL"
                width="100%"
                height="100%"
                horizontal="false"
                vertical="true"
            >
                <Button
                    id="idNavBackButton1_UOBL"
                    icon="sap-icon://nav-back"
                    press="Onpressback1"
                />
                <VBox
                    id="idMainVBox_UBOL"
                    alignItems="Center"
                    justifyContent="Center"
                    class="centerContent"
                >
                    <Title
                        id="Title_UBOL"
                        text="HU Details"
                        class="TitleSecondPage"
                        level="H2"
                    />
                    <f:SimpleForm
                        id="simpleForm1_UBOL"
                        layout="ResponsiveGridLayout"
                        editable="true"
                        columnsL="2"
                        columnsM="1"
                    >
                        <!-- Delivery Field -->
                        <Label
                            id="idDeliveryLabel1_UOBL"
                            text="Delivery"
                            class="sapUiTinyMarginEnd"
                        />
                        <Input
                            id="idDeliveryInput_UOBL"
                            placeholder="Enter Delivery..."
                            maxLength="25"
                            width="200px"
                        />

                        <Label
                            id="idItemNoLabel_UOBL"
                            text="Item No"
                            class="sapUiTinyMarginEnd"
                        />
                        <Input
                            id="idItemNoInput_UOBL"
                            placeholder="Enter Item No..."
                            maxLength="25"
                            width="200px"
                        />

                        <!-- Vendor Field -->
                        <Label
                            id="idVendorLabel_UOBL"
                            text="Vendor"
                        />
                        <Input
                            id="idVendorInput_UOBL"
                            placeholder="Enter Vendor..."
                            maxLength="25"
                            width="200px"
                        />

                        <!-- No. of HU Field -->
                        <Label
                            id="idNoOfHULabel_UOBL"
                            text="No. of HU"
                        />
                        <Input
                            id="idNoOfHUInput_UOBL"
                            placeholder="Enter No. of HU..."
                            maxLength="25"
                            width="200px"
                        />

                        <!-- HU Field -->
                        <Label
                            id="idHULabel_UOBL"
                            text="HU"
                        />
                        <VBox
                            id="idHUVBox_UBOL"
                        >
                            <Input
                                id="idHUInput_UOBL"
                                placeholder="Enter HU..."
