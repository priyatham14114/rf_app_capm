                                width="200px"
                            />

                            <!-- HU List and New HU Buttons Positioned Below HU Field -->
                            <HBox
                                id="idHUButtonHBox1_UOBL"
                                class="sapUiSmallMarginTop"
                              
                            >
                                <Button
                                    id="idHUListButton_UOBL"
                                    text="HU List"
                                    press="onHUListPress"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Button
                                    id="idNewHUButton_UOBL"
                                    text="New HU"
                                    class="sapUiTinyMarginEnd"
                                    press="onNewHUPress"
                                />
                                <Button
                                    id="idGRButton_UOBL"
                                    text="GR"
                                    press="onGRRPress"
                                />
                            </HBox>
                            <HBox
                                id="idHUButtonHBox2_UOBL"
                                class="sapUiSmallMarginTop"
                                justifyContent="Start"
                            >
                                <Button
                                    id="idHUChangeButton_UOBL"
                                    text="HU Change"
                                    press="onHUContentPress"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Button
                                    id="idUnloadButtoninpage2_UOBL"
                                    text="Unload"
                                    press="onHUHierarchyPress"
                                />
                            </HBox>
                        </VBox>
                    </f:SimpleForm>
                </VBox>
            </ScrollContainer>

            <ScrollContainer
                id="page3billofLading_UOBL"
                width="100%"
                height="100%"
                horizontal="false"
                vertical="true"
            >
                <HBox
                    id="idMainHBox_UBOL"
                    width="100%"
                    justifyContent="SpaceBetween"
                    alignItems="Center"
                >
                    <Button
                        id="idNavBackButton2_UOBL"
                        icon="sap-icon://nav-back"
                        press="Onpressback2"
                    />
                    <Title
                        id="idTitle_UBOL"
                        text="HU List"
                        class="TitleSecondPage"
                        level="H1"
                        textAlign="Center"
                    />
                    <!-- This empty Text element helps push the Title to the center -->
                    <Text
                        id="idEmptyText_UBOL"
                        text=""
                        width="32px"
                    />
                </HBox>
                <VBox
                    id="idMainVBox_UBOLL"
                    alignItems="Center"
                    justifyContent="Center"
                    class="centerContent"
                >
                    <Table
                        id="idHuDetailsTable_UOBL"
                        inset="false"
                        items="{/productData}"
                        growing="true"
                        growingScrollToLoad="true"
                        mode="None"
                    >
                        <columns>
                            <Column
                                id="idSerialNoColumn_UBOL"
                                width="17%"
                            >
                                <Text
                                    id="idSerialNoHeader_UBOL"
                                    text="S.No"
                                />
                            </Column>
                            <Column
                                id="idHuNoColumn_UBOL"
                            >
                                <Text
                                    id="idHuNoHeader_UBOL"
                                    text="HU No"
                                />
                            </Column>
                            <Column
                                id="idProductColumn_UBOL"
                            >
                                <Text
                                    id="idProductHeader_UBOL"
                                    text="Product"
                                />
                            </Column>
                            <Column
                                id="idProductDescriptionColumn_UBOL"
                            >
                                <Text
                                    id="idProductDescriptionHeader_UBOL"
                                    text="Product Description"
                                />
                            </Column>
                            <Column
                                id="idBatchColumn_UBOL"
                            >
                                <Text
                                    id="idBatchHeader_UBOL"
                                    text="Batch"
                                />
                            </Column>
                        </columns>
                        <items>
                            <ColumnListItem id="colum_UBOL">
                                <Text
                                    id="idSerialNoItem_UBOL"
                                    text="{SerialNo}"
                                />
                                <Text
                                    id="idHuNoItem_UBOL"
                                    text="{HuNo}"
                                />
                                <Text
                                    id="idProductItem_UBOL"
                                    text="{Product}"
                                />
                                <Text
                                    id="idProductDescriptionItem_UBOL"
                                    text="{ProductDescription}"
                                />
                                <Text
                                    id="idBatchItem_UBOL"
                                    text="{Batch}"
                                />
                            </ColumnListItem>
                        </items>
                    </Table>
                </VBox>
            </ScrollContainer>
        


