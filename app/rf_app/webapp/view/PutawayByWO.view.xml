<mvc:View
    xmlns:table="sap.ui.table"
    controllerName="com.app.rfapp.controller.PutawayByWO"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    height="100%"
>
    <tnt:ToolPage id="idToolPagePutawayByWO">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeaderPutawayByWO">
                <Image
                    id="_IDGenImage1PutawayByWO"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparatorPutawayByWO"
                />
                <ToolbarSpacer id="idToolbarSpacerPutawayByWO">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData1PutawayByWO"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar1PutawayByWO"
                    displaySize="XS"
                    press="onAvatarPressed"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <ScrollContainer
                id="idPage1ScannerFormBox"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="true"
            >
                <VBox
                    id="idWarehouseOderNoformBox"
                    class="putawayContainer_PBYWO"
                >
                    <HBox
                        id="idHBoxWarhouseTitle"
                        class="warehouseOrderTitle_PBYWO"
                        alignItems="Center"
                    >
                        <Button
                            id="IdBackBtnToMainPage"
                            icon="sap-icon://nav-back"
                            type="Transparent"
                            press="onPressBackBtnScanerFormBox"
                        />
                        <Title
                            id="idWarehouseOrder"
                            text="Warehouse Order:"
                            level="H2"
                        />
                    </HBox>
                    <VBox
                        id="idVBoxWarehouseON"
                        class="formContainer_PBYWO"
                    >
                        <VBox
                            id="idVBOXwarhouse"
                            class="formBox_PBYWO"
                        >
                            <Label
                                id="idLableWarehouse"
                                text="Warehouse Order Number:"
                                class="formLabel_PBYWO"
                            />
                            <HBox
                                id="idHBoxWarehouseorderno"
                                alignItems="Center"
                                class="formHBox"
                                justifyContent="Start"
                                width="auto"
                            >
                                <Input
                                    id="idInputWarhouseOrderNo"
                                    placeholder="Enter Warehouse order no..."
                                    class="formInput"
                                    liveChange="onLiveChange"
                                    width="120%"
                                />
                                <ndc:BarcodeScannerButton
                                    id="sampleBarcodeScannerButton_PBYWO"
                                    scanSuccess="onScanSuccess"
                                    scanFail="onScanError"
                                    inputLiveUpdate="onScanLiveupdate"
                                    dialogTitle="Enter Warehouse Order Number:"
                                    class="Scannerbtn_PTBYWO"
                                />
                                <Button
                                    id="idSubmitBtnWarhouseON_PBYWO"
                                    text="Submit"
                                    press="onSubmitPress"
                                />
                            </HBox>
                        </VBox>
                    </VBox>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idPage2HUDetails_PBYWO"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <Button
                    id="idBackBtn_PBYWO"
                    press="onBackPressHUDetails"
                    class="sapUiTinyMarginBottom"
                    icon="sap-icon://nav-back"
                    type="Transparent"
                />
                <VBox
                    id="idSecondPageContainer"
                    class="Page2HUDetailsContainer_PBYWO"
                    justifyContent="Center"
                >
                    <Title
                        id="idSecondPageTitle"
                        text="Second Page"
                        level="H2"
                        class="Page2formHeaderTitle_PBYWO"
                    />
                    <VBox
                        id="idSecondBox"
                        class="Page2FormBox_PBYWO"
                    >
                        <form:SimpleForm
                            id="idSimpleFormSecondPage"
                            layout="ResponsiveGridLayout"
                            labelSpanL="4"
                            labelSpanM="4"
                            columnsL="1"
                            columnsM="1"
                        >
                            <Label
                                id="idSecondPageLable1"
                                text="SrSc HU"
                                class="labelStyleSecondPage"
                            />
                            <Input
                                id="idSrScHuInput_PBYWO"
                                width="10rem"
                            />
                            <Label
                                id="idSecondPageLable2"
                                text="Dst Bin"
                                class="labelStyleSecondPage"
                            />
                            <Input
                                id="idStorageTypeInput_PBYWO"
                                placeholder="Stor type"
                                width="5rem"
                                class="sapUiTinyMarginEnd"
                            />
                            <Label
                                id="idSecondPageLable3"
                                text="Sec"
                                class="labelStyleSecondPage"
                            />
                            <Input
                                id="idSectionInput_PBYWO"
                                placeholder="Sec"
                                width="5rem"
                                class="sapUiTinyMarginEnd"
                            />
                            <Label
                                id="idSecondPageLable4"
                                text="Bin"
                                class="labelStyleSecondPage"
                            />
                            <Input
                                id="idBinInputSecondPage"
                                placeholder="Bin"
                                width="7rem"
                            />
                            <HBox
                                id="idHBoxSecondPage"
                                class="buttonContainer"
                                justifyContent="End"
                            >
                                <Button
                                    id="idBtnHuListBtn_PBYWO"
                                    text="HuList"
                                    press="onPressHUListTable"
                                    class="HUListBtn_PBYWO"
                                />
                            </HBox>
                        </form:SimpleForm>
                    </VBox>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idPage3HUListTable_PBYWO"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <Button
                    id="idBackBtn2_PBYWO"
                    press="onBackPressHUListTable"
                    class="sapUiTinyMarginBottom"
                    icon="sap-icon://nav-back"
                    type="Transparent"
                />
                <VBox id="idVBoxHUListable_PBYWO">
                    <Toolbar
                        id="TB4_PBYWO"
                        class="HeaderToolBar"
                    >
                        <Title
                            id="Title1455896_PBYWO"
                            text="HU NUMBERS LIST:"
                        />
                        <ToolbarSpacer id="IDGenToolbar345_PBYWO" />
                        <Button
                            id="idBtnNewHUNumberBtn_PBYWO"
                            icon="sap-icon://add"
                            press="onPressNewHUNumberBtn"
                            tooltip="Add New HUNumber"
                        />
                        <Button
                            id="idRefrehshBtnReservations_PBYWO"
                            press="onRefreshReservations"
                            icon="sap-icon://synchronize"
                        />
                        <SearchField
                            id="idSearchFieldFromHUNumbers_PBYWO"
                            liveChange="onLiveHUNumbersList"
                            placeholder="Search Anything..."
                            width="25%"
                        />
                    </Toolbar>
                    <Toolbar id="idToolBarHeading_PBYWO">
                        <Title
                            id="idTextTitle_PBYWO"
                            text="Table Contents:-"
                        />
                    </Toolbar>
                    <Table
                        id="idHuListTable_PBYWO"
                        width="100%"
                    >
                        <columns>
                            <Column id="IDGenColumn001_PBYWO">
                                <Text
                                    id="IDGenText001_PBYWO"
                                    text="Serial No:"
                                    class="HedingNameColor"
                                />
                            </Column>
                            <Column id="IDGenColumn002_PBYWO">
                                <Text
                                    id="IDGenText002_PBYWO"
                                    text="HU Number:"
                                    class="HedingNameColor"
                                />
                            </Column>
                            <Column id="IDGenColumn003_PBYWO">
                                <Text
                                    id="IDGenText003_PBYWO"
                                    text="Product:"
                                    class="HedingNameColor"
                                />
                            </Column>
                            <Column id="IDGenColumn004_PBYWO">
                                <Text
                                    id="IDGenText004_PBYWO"
                                    text="Product Description:"
                                    class="HedingNameColor"
                                />
                            </Column>
                            <Column id="IDGenColumn005_PBYWO">
                                <Text
                                    id="IDGenText005_PBYWO"
                                    text="Quantity:"
                                    class="HedingNameColor"
                                />
                            </Column>
                            <Column id="IDGenColumn006_PBYWO">
                                <Text
                                    id="IDGenText006_PBYWO"
                                    text="Units of Measurement:"
                                    class="HedingNameColor"
                                />
                            </Column>
                        </columns>
                        <items>
                            <ColumnListItem
                                id="IDGenHuNumbersColumnListItem1_PBYWO"
                                class="AlternateRowColor"
                            >
                                <cells>
                                    <Text
                                        id="IDGenHunoListText1_PBYWO"
                                        text="{Vendorname}"
                                    />
                                    <Text
                                        id="IDGenHunoListText2_PBYWO"
                                        text="{Vendornumber}"
                                    />
                                    <Text
                                        id="IDGenHunoListText3_PBYWO"
                                        text="{Drivername}"
                                    />
                                    <Text
                                        id="IDGenHunoListText4_PBYWO"
                                        text="{Drivernumber}"
                                    />
                                    <Text
                                        id="IDGenHunoListText5_PBYWO"
                                        text="{Vehicletype}"
                                    />
                                    <Text
                                        id="IDGenHunoListText6_PBYWO"
                                        text="{Vehiclenumber}"
                                    />
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idPage4NewHUNumber_PBYWO"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <Button
                    id="idBackButton_PBYWO"
                    press="onBackBtnPressNewHUNumber"
                    icon="sap-icon://nav-back"
                    type="Transparent"
                    class="sapUiTinyMarginBottom"
                />
                <form:SimpleForm
                    id="SimpleFormToolbar_PBYWO"
                    editable="true"
                    layout="ResponsiveGridLayout"
                    labelSpanXL="4"
                    labelSpanL="3"
                    labelSpanM="4"
                    labelSpanS="12"
                    adjustLabelSpan="false"
                    emptySpanXL="0"
                    emptySpanL="4"
                    emptySpanM="0"
                    emptySpanS="0"
                    columnsXL="2"
                    columnsL="1"
                    columnsM="1"
                    singleContainerFullSize="false"
                    ariaLabelledBy="Title1"
                >
                    <form:toolbar>
                        <Toolbar
                            id="TB1_PBYWO"
                            class="HeaderToolBar"
                        >
                            <Title
                                id="Title1_PBYWO"
                                text="NEW HU NUMBER:"
                            />
                        </Toolbar>
                    </form:toolbar>
                    <form:content>
                        <Toolbar
                            id="IDGenToolbar1_PBYWO"
                            ariaLabelledBy="Title2"
                        >
                            <Title
                                id="Title2_PBYWO"
                                text="Add New HU Number:"
                            />
                        </Toolbar>
                        <Label
                            id="IDGenLabel1_PBYWO"
                            text="Product"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <Input
                            id="idInputProduct_PBYWO"
                            width="80%"
                        />
                        <Label
                            id="IDGenLabel2_PBYWO"
                            text="Quantity"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <HBox id="idHBoxPutaway">
                            <Input
                                id="idInputQty_PBYWO"
                                width="auto"
                                class="sapUiTinyMarginEnd"
                            />
                            <Label
                                id="idTextUom_PBYWO"
                                text="UOM:"
                                class="UnitsofMeasure"
                                required="true"
                            />
                            <Input id="idInputUOM" width="auto"/>
                        </HBox>
                        <Label
                            id="IDGenLabel3_PBYWO"
                            text="Packaging Matirial"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <Input
                            id="idPackagingMaterialPutawayByWO"
                            width="80%"
                        />
                        <Label
                            id="IDGenLabel4_PBYWO"
                            text="Batch"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <Input
                            id="idBatch_PBYWO"
                            width="80%"
                        />
                        <Label
                            id="IDGenLabel5_PBYWO"
                            text="Best Before Date"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <DatePicker
                            id="idDatePickerBBD_PBYWO"
                            width="80%"
                        />
                        <Label
                            id="IDGenLabel6_PBYWO"
                            text="Manufacturing Date"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <Input
                            id="idManufacturingDate_PBYWO"
                            width="80%"
                        />
                        <Label
                            id="IDGenLabel7_PBYWO"
                            text="Expiry Date"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <Input
                            id="idExpiryDate_PBYWO"
                            width="80%"
                        />
                        <Label
                            id="IDGenLabel8_PBYWO"
                            text="COO"
                            required="true"
                            class="ClassLableroot1"
                        />
                        <Input
                            id="idCOO_PBYWO"
                            width="80%"
                        />
                        <HBox
                            id="IDGenHBox2_PBYWO"
                            justifyContent="End"
                        >
                            <Button
                                id="IDGenButton5_PBYWO"
                                text="Enter"
                                press="onEnterNewHUNUmberPress"
                                class="NewHUNUmber"
                            />
                            <Button
                                id="IDGenButton6_PBYWO"
                                text="Clear"
                                press="onClearNewHUNumberPress"
                                class="ClearNewHUNumber"
                            />
                        </HBox>
                    </form:content>
                </form:SimpleForm>
            </ScrollContainer>
            <ScrollContainer
                id="idPage5VerifyHUNumber_PBYWO"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <Button
                    id="idBackBtnPressVerifyHUNumber_PBYWO"
                    press="onBackPressVerifyHUNumber"
                    icon="sap-icon://nav-back"
                    type="Transparent"
                    class="backButtonMargin"
                />
                <VBox
                    id="idVerifyHuNumberContainer_PBYWO"
                    class="verifyHuNumberContainer"
                    justifyContent="Center"
                >
                    <Title
                        id="idVerifyHuNumberTitle_PBYWO"
                        text="Verify HU Number"
                        level="H2"
                        class="Page5formHeaderVerifyHUNum_PBYWO"
                    />
                    <VBox
                        id="idVerifyHuNumberBox_PBYWO"
                        class="verifyHuNumberBox"
                    >
                        <form:SimpleForm
                            id="idSimpleFormVerifyHuNumber_PBYWO"
                            layout="ResponsiveGridLayout"
                            labelSpanL="4"
                            labelSpanM="4"
                            columnsL="1"
                            columnsM="1"
                        >
                            <Label
                                id="idHUNumber_PBYWO"
                                text="HU Number"
                                class="formLabel_PBYWO"
                            />
                            <Input
                                id="idHuInput_PBYWO"
                                class="formInput"
                            />
                            <Label
                                id="idDeliverynumber_PBYWO"
                                text="Delivery Number"
                                class="formLabel_PBYWO"
                            />
                            <Input
                                id="idDeliveryInput_PBYWO"
                                class="formInput"
                            />
                            <Label
                                id="idItemNumber_PBYWO"
                                text="Item Number"
                                class="formLabel_PBYWO"
                            />
                            <Input
                                id="idItemNoInput_PBYWO"
                                class="formInput"
                            />
                            <HBox
                                id="idVBoxbuttonContainer_PBYWO"
                                class="buttonContainer"
                                justifyContent="End"
                            >
                                <Button
                                    id="idGrBtn_PBYWO"
                                    text="GR"
                                    press="onGRBtnPressVerifyHUNumber"
                                    class="GRandUPLOADBtns_PBYWO"
                                />
                                <Button
                                    id="idUnloadBtn_PBYWO"
                                    text="Unload"
                                    press="onUnloadPressVerifyHUNumber"
                                    class="GRandUPLOADBtns_PBYWO"
                                />
                            </HBox>
                        </form:SimpleForm>
                    </VBox>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idPage6GoodsReceiptGR_PBYWO"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <Button
                    id="idBackBtnGoodsReceipt_PBYWO"
                    press="onBackPressGoodsReceipt"
                    icon="sap-icon://nav-back"
                    type="Transparent"
                />
                <VBox
                    id="idMainVBoxGR_PBYWO"
                    class="MainVBoxGR"
                    justifyContent="End"
                >
                    <Title
                        id="idTitleGoodsReceipt_PBYWO"
                        text="Goods Receipt"
                        level="H2"
                        class="formHeaderGR"
                    />
                    <VBox
                        id="idGoodsReceiptFormBox_PBYWO"
                        class="GoodsReceiptFormBox"
                        justifyContent="End"
                    >
                        <form:SimpleForm
                            id="idSimpleFormGR_PBYWO"
                            layout="ResponsiveGridLayout"
                            labelSpanL="4"
                            labelSpanM="4"
                            columnsL="1"
                            columnsM="1"
                        >
                            <Label
                                id="idPage6Label1_PBYWO"
                                text="Product"
                                class="labelStyleGR"
                            />
                            <Input
                                id="idProductInput_PBYWO"
                                width="12rem"
                            />
                            <Label
                                id="idPage6Label2_PBYWO"
                                text="Quantity"
                                class="labelStyleGR"
                            />
                            <Input
                                id="idQuantityInput_PBYWO"
                                width="12rem"
                            />
                            <HBox
                                id="idVBoxbuttonGR_PBYWO"
                                class="buttonContainer"
                                justifyContent="End"
                            >
                                <Button
                                    id="idGrBtnforPage6_PBYWO"
                                    text="GR"
                                    press="onGRBtnPress"
                                    class="formButton"
                                />
                            </HBox>
                        </form:SimpleForm>
                    </VBox>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idPage7UnLoadPage_PBYWO"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <Button
                    id="idBackBtnUnLoad_PBYWO"
                    press="onBackPressUnLoadPage"
                    icon="sap-icon://nav-back"
                    type="Transparent"
                />
                <VBox
                    id="idMainUnLoadPage_PBYWO"
                    class="MainVBoxUnLoad"
                    justifyContent="End"
                >
                    <Title
                        id="idTitleUnLoadPage_PBYWO"
                        text="Un Load Page"
                        level="H2"
                        class="formHeaderUnLoad"
                    />
                    <VBox
                        id="idGoodsUnLoadPageBox_PBYWO"
                        class="UnLoadPageFormBox"
                        justifyContent="End"
                    >
                        <form:SimpleForm
                            id="idSimpleFormUnLoad_PBYWO"
                            layout="ResponsiveGridLayout"
                            labelSpanL="4"
                            labelSpanM="4"
                            columnsL="1"
                            columnsM="1"
                        >
                            <Label
                                id="idPage7Label1_PBYWO"
                                text="Product"
                                class="labelStyleUnLoad"
                            />
                            <Input
                                id="idUnLoadProductInput_PBYWO"
                                width="12rem"
                            />
                            <Label
                                id="idPage7Label2_PBYWO"
                                text="Quantity"
                                class="labelStyleUnLoad"
                            />
                            <Input
                                id="idUnLoadQuantityInput_PBYWO"
                                width="12rem"
                            />
                            <HBox
                                id="idVBoxbuttonUnLoad_PBYWO"
                                class="buttonContainer"
                                justifyContent="End"
                            >
                                <Button
                                    id="idUnLoadBtnforPage7_PBYWO"
                                    text="UnLoad"
                                    press="onUnLoadBtnPress"
                                />
                            </HBox>
                        </form:SimpleForm>
                    </VBox>
                </VBox>
            </ScrollContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
