<mvc:View
    xmlns:form="sap.ui.layout.form"
    controllerName="com.app.rfapp.controller.SerialNumberLocation"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    height="100%"
>
    <tnt:ToolPage id="idToolPageSNL">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeaderSNL">
                <Button
                    id="idSNLfirstBackBtn"
                    icon="sap-icon://nav-back"
                    press="onSNLfirxtBackBtnPress"
                    visible="true"
                />
                 <Button
                    id="idSNLSecondBackBtn"
                    icon="sap-icon://nav-back"
                    press="onSNLSecondBackBtnPress"
                    visible="false"
                />
                <Button
                    id="idSNLthirdBackBtn"
                    icon="sap-icon://nav-back"
                    press="onSNLthirdBackBtnPress"
                    visible="false"
                />
                <Button
                    id="idSNLfourthBackBtn"
                    icon="sap-icon://nav-back"
                    press="onSNLfourthBackBtnPress"
                    visible="false"
                />
                <Button
                    id="idSNLfifthBackBtn"
                    icon="sap-icon://nav-back"
                    press="onSNLfifthBackBtnPress"
                    visible="false"
                />
                <Image
                    id="_IDGenImage1SNL"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparatorSNL"
                />
                <ToolbarSpacer id="idToolbarSpacerSNL">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData1SNL"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar1SNL"
                    displaySize="XS"
                    press="onSNLAvatarPressed"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <ScrollContainer
                id="idSNLFirstSC"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="true"
            >
                <form:SimpleForm
                    layout="ResponsiveGridLayout"
                    labelSpanL="4"
                    labelSpanM="4"
                    emptySpanL="1"
                    emptySpanM="1"
                    columnsL="2"
                    columnsM="1"
                    columnsS="1"
                >
                    <VBox
                        alignContent="Center"
                        alignItems="Center"
                    >


                    <Title text="Serial Number Location"></Title>
                        <VBox class="sapUiTinyMarginBottom"></VBox>

                        <HBox
                            id="idSNLHbox"
                            class="sapUiMediumMargin"
                        >
                            <Label
                                id="idSNLroductLabel"
                                text="Product"
                                width="100px"
                                 class="sapUiTinyMargin"

                            />
                            <Input
                                id="idSNLproductInput"
                                value=""
                                liveChange="onSNLproductLiveChange"
                            />
                            <Button
                                id="idSNLproductScannerBtn"
                                icon="sap-icon://bar-code"
                                press="onSNLproductBarcodeScanner"
                            />
                        </HBox>
                    </VBox>
                </form:SimpleForm>
            </ScrollContainer>
            <ScrollContainer
                id="idSNLsecondSC"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <form:SimpleForm
                    id="simpleform2"
                    layout="ResponsiveGridLayout"
                    labelSpanL="4"
                    labelSpanM="4"
                    emptySpanL="1"
                    emptySpanM="1"
                    columnsL="2"
                    columnsM="1"
                    columnsS="1"
                >
                    <VBox alignContent="Center" alignItems="Center">
                        <HBox>
                            <Label
                                id="idSNLPro"
                                text="Product"
                                width="100px"

                                 class="sapUiTinyMargin"

                            />
                            <Input
                                id="idSNLProInput"
                                value=""
                            />
                        </HBox>
                        <HBox>
                            <Label
                                id="idSNLSno"
                                text="Serial Number"
                                width="100px"

                                 class="sapUiTinyMargin"

                            />
                            <Input
                                id="idSNLsnoInput"
                                value=""
                                liveChange="onSNLsnoLiveChange"
                            />
                        </HBox>
                    </VBox>
                </form:SimpleForm>
            </ScrollContainer>
            <ScrollContainer
                id="idSNLthirdSC"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <form:SimpleForm
                    id="simpleform3"
                    layout="ResponsiveGridLayout"
                    labelSpanL="4"
                    labelSpanM="4"
                    emptySpanL="1"
                    emptySpanM="1"
                    columnsL="2"
                    columnsM="1"
                    columnsS="1"
                >
                    <VBox alignContent="Center" alignItems="Center">
                        <HBox>
                            <Label
                                id="idSNLPro1"
                                text="Product"
                                width="100px"
                            />
                            <Input
                                id="idSNLProInput1"
                                value=""
