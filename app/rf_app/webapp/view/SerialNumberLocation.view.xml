                    labelSpanL="4"
                    labelSpanM="4"
                    emptySpanL="1"
                    emptySpanM="1"
                    columnsL="2"
                    columnsM="1"
                    columnsS="1"
                >
                    <VBox
                        alignItems="Center"
                        justifyContent="Center"
                        width="100%"
                    >
                        <VBox class="sapUiLargeMarginBottom" />
                        <HBox>
                            <Label
                                id="idSNLBinLabel"
                                text="Bin"
                                width="90Px"
                            />
                            <Input
                                id="idSNLBinInput"
                                value=""
                            />
                            <Label
                                text=""
                                width="30px"

                            />
                            <Label
                                id="idSNLStorTypeLabel"
                                text="StorageType"
                                width="90Px"
                            />
                            <Input
                                id="idSNLStoreTypeInput"
                                value=""
                            />
                        </HBox>

                        <HBox>
                            <Label
                                id="idSNLQtyLabel"
                                text="Qty"
                                width="90Px"
                            />
                            <Input
                                id="idSNLQtyWInput"
                                value=""
                            />
                             <Label
                                text=""
                                width="30px"

                            />
                            <Label
                                id="idSNLStorSecLabel"
                                text="Storage Sec"
                                width="90Px"
                            />
                            <Input
                                id="idSNLStorSecInput"
                                value=""
                            />
                        </HBox>
                        <HBox>
                            <Label
                                id="idSBQNoOfHuLabel"
                                text="No of Hu's"
                                width="90Px"
                            />
                            <Input
                                id="idSNLNoOfHuInput"
                                value=""
                            />
                             <Label
                                text=""
                                width="30px"

                            />
                            <Label
                                id="idSNLBinAisleLabel"
                                text="Bin Aisle"
                                width="90Px"
                            />
                            <Input
                                id="idSNLBinAisleInput"
                                value=""
                            />
                        </HBox>
                        <HBox>
                            <Label
                                id="idSBQMaxWLabel" 
                                text="MaxW"
                                width="90Px"
                            />
                            <Input
                                id="idSNLMaxWInput"
                                value=""
                            />
                            
                            <Text text="KG"/>
                             <Label
                                text=""
                                width="30px"

                            />
                            <Label
                                id="idSNLBinLevelLabel"
                                text="Bin Level"
                                width="90Px"
                            />
                            <Input
                                id="idSNLBinLevelInput"
                                value=""
                            />
                        </HBox>
                        <HBox>
                            <Label
                                id="idSBQMaxVLabel"
                                text="MaxV"
                                width="90Px"
                            />
                            <Input
                                id="idSNLMaxVInput"
                                value=""
