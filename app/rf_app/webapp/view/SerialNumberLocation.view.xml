                        </HBox>
                        <HBox>
                            <Label
                                id="idSNLSno1"
                                text="Serial Number"
                                width="100px"
                            />
                            <Input
                                id="idSNLsnoInput1"
                                value=""
                               
                            />
                        </HBox>

                        <VBox class="sapUiMediumMarginBottom"></VBox>

 <HBox
                                alignItems="Center"
                                justifyContent="Center"
                                width="100%"
                            >
                                <Table
                                    id="idSNLTable"
                                    growing="true"
                                    growingScrollToLoad="true"
                                    mode="None"
                                    width="100%"
                                >
                                    <columns>
                                        <Column id="idSNLBinNoColumn">
                                            <Text
                                                id="idSNLBinNoText"
                                                text="Bin No"
                                            />
                                        </Column>
                                        <Column id="idSNLBinColumn">
                                            <Text
                                                id="idSNLBinText"
                                                text="Bin"
                                            />
                                        </Column>
                                        <Column id="idSNLColumn">
                                            <Text
                                                id="idSNLText"
                                                text="P"
                                            />
                                        </Column>
                                        <Column id="idSNLPcColumn">
                                            <Text
                                                id="idSNLPcText"
                                                text="pc"
                                            />
                                        </Column>
                                    </columns>

                                    <items>
                                        <ColumnListItem
                                            id="idGenSNLColumnListItem"
                                        >
                                            <cells>
                                                <Text
                                                    id="idSNLBinNoCell"
                                                    text=""
                                                />
                                                <Text
                                                    id="idSNLBinCell"
                                                    text=""
                                                />
                                                <Text
                                                    id="idSNLpCell"
                                                    text=""
                                                />
                                                <Text
                                                    id="idSNLpc"
                                                    text=""
                                                />
                                            </cells>
                                        </ColumnListItem>
                                    </items>
                                </Table>
                            </HBox>
 <VBox class="sapUiLargeMarginBottom" />
                        <HBox
                            alignItems="Center"
                            justifyContent="Center"
                            width="100%"
                        >
                            <Button
                                id="idSNLprdDe"
                                text="Prd De"
                                class="sapUiTinyMargin"
                                press="onSNLPreDePress"
                            />

                            <Button
                                id="idSNLBinDe"
                                text="Bin De"
                                class="sapUiTinyMargin"
                                press="onSNLBinDePress"
                            />
                            
                        </HBox>
                    </VBox>
                </form:SimpleForm>
            </ScrollContainer>
             <ScrollContainer
                id="idSNLfourthSC"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <form:SimpleForm
                    id="simpleform4"
                    layout="ResponsiveGridLayout"
                    labelSpanL="4"
                    labelSpanM="4"
                    emptySpanL="1"
                    emptySpanM="1"
                    columnsL="2"
                    columnsM="1"
                    columnsS="1"
                >
                    <VBox
                        alignItems="Center"
                        justifyContent="Center"
                        width="100%"
                    >
                        <Title text="Product Description" />
                        <VBox class="sapUiLargeMarginBottom" />
                        <HBox>
                            <Label
                                id="idSNLSnoLabel2"
                                text="Serial Number"
                                width="100Px"
                            />
                            <Input
                                id="idSNLSnoInput"
                                value=""
                            />
                            </HBox>
                        <HBox>
                            <Label
                                id="idSNLEANLabel"
                                text="EAN/UPC"
                                width="70Px"
                            />
                            <Input
                                id="idSNLEANInput"
                                value=""
                            />
                            <Input
                                id="idSNLEANInput2"
                                value=""
                            />
                        </HBox>

                        <HBox>
                            <Label
                                id="idSNLTotWLabel"
                                text="TotW"
                                width="70Px"
                            />
                            <Input
                                id="idSNLTotWInput"
                                value=""
                            />
                            <Input
                                id="idSNLTotWInput2"
                                value="KG"
                            />
                        </HBox>

                        <HBox>
                            <Label
                                id="idSNLTotVLabel"
                                text="TotV"
                                width="70Px"
                            />
                            <Input
                                id="idSNLTotVInput"
                                value=""
                            />
                            <Input
                                id="idSNLTotVInput2"
                                value="L"
                            />
                        </HBox>
                    </VBox>
                </form:SimpleForm>
            </ScrollContainer>
            <ScrollContainer
                id="idSNLFifthSC"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <form:SimpleForm
